<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <NullableContextOptions>enable</NullableContextOptions>
    <WarningsAsErrors>CS8597;CS8600;CS8601;CS8602;CS8603;CS8604;CS8618;CS8622;CS8625</WarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <Version>0.2.0</Version>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>RuTracker.Client</PackageId>
    <PackageProjectUrl>https://github.com/ilyalatt/RuTracker.Client</PackageProjectUrl>
    <PackageIconUrl>https://rutracker.org/favicon.ico</PackageIconUrl>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>rutracker.org client</Description>
    <Authors>ilyalatt</Authors>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="0.13.0" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.6.0" />
  </ItemGroup>

</Project>
